{
  "name": "DWUnique",
  "description": "Filter out those pesky DW duplicates!",
  "scripts": {
  },
  "env": {
    "PYTHONUNBUFFERED": {
      "value": "TRUE"
    },
    "DEBUG": {
      "value": "TRUE"
    },
    "MONGODB_URI": {
      "required": true
    },
    "SPOTIPY_CLIENT_ID": {
      "required": true
    },
    "SPOTIPY_CLIENT_SECRET": {
      "required": true
    },
    "SPOTIPY_REDIRECT_URI": {
      "required": true
    },
    "DWUNIQUE_REFRESH_TOKEN": {
      "required": true
    },
    "TWILIO_ACCOUNT_SID": {
      "required": true
    },
    "TWILIO_AUTH_TOKEN": {
      "required": true
    },
    "TWILIO_NUMBER": {
      "required": true
    },
    "SECRET_KEY": {
      "generator": "secret"
    }
  },
  "formation": {
  },
  "addons": [
    "mongolab",
    "scheduler"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ]
}
